{
    "name": "freeiamhq/discord-manager",
    "description": "Connector between Freeiam & Discord.",
    "homepage": "https://www.freeiam.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Micha Vie",
            "email": "micha@freeiam.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.0",
        "laravel-zero/framework": "^8.4",
        "team-reflex/discord-php": "^5.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["discord-manager"]
}
